	<queryString>
		<![CDATA[select t.nombre as transportista, A.id as id, A.nombre as nombreArticulo, 'CAJAS' as unidad,
sum(AP.cantAtendida*EU.factor_conversion)/ (select EEUU.factor_conversion from equivalencia_unidades as EEUU
where EEUU.unidad_id = 3 and A.id = EEUU.articulo_id)
as cantidad, sum(AP.ImporteAtendido)as total
from articulos_pedido as AP
inner join remito as R on AP.id_pedido = R.id_pedido
inner join articulo as A on A.id = AP.id_articulo
inner join unidad as U on U.id= A.unidad
inner join pedido as P on P.id= AP.id_pedido
inner join equivalencia_unidades as EU on EU.id = AP.equivalenciaUnidades_id
inner join reparto as RR on RR.id = R.id_reparto
right outer join transportista as T on RR.id_transportista = T.id
WHERE 
	 $X{IN,T.id,transportistas} and RR.fecha between $P{desde}  and  $P{hasta} 
GROUP BY transportista,A.id]]>
	</queryString>
